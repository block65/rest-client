{
  "name": "@block65/rest-client",
  "version": "1.0.3",
  "private": false,
  "description": "Block65 REST client",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "node": "./dist/src/node.js",
      "default": "./dist/src/browser.js"
    }
  },
  "main": "./dist/src/node.js",
  "browser": "./dist/src/browser.js",
  "types": "./dist/src/node.js",
  "typesVersions": {
    "*": {
      "node": [
        "./dist/src/node.js"
      ],
      "default": [
        "./dist/src/browser.js"
      ]
    }
  },
  "files": [
    "dist/{src,lib}"
  ],
  "scripts": {
    "clean": "make clean",
    "test": "NODE_OPTIONS=--experimental-vm-modules yarn jest"
  },
  "dependencies": {
    "@block65/custom-error": "^11.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@block65/eslint-config": "7.0.4",
    "@jest/globals": "^28.1.0",
    "@types/node": "^17.0.32",
    "@typescript-eslint/eslint-plugin": ">=5.23.0",
    "@typescript-eslint/parser": ">=5.23.0",
    "eslint": ">=8.15.0",
    "eslint-plugin-formatjs": ">=3.1.1",
    "eslint-plugin-import": ">=2.26.0",
    "eslint-plugin-prettier": ">=4.0.0",
    "get-port": "^6.1.2",
    "got": "^12.3.1",
    "jest": "^28.1.0",
    "node-fetch": "^3.2.4",
    "prettier": "^2.6.2",
    "type-fest": "^2.12.2",
    "typescript": "^4.8.2"
  },
  "peerDependencies": {
    "got": "^12.3.1",
    "node-fetch": "^3.2.4"
  },
  "peerDependenciesMeta": {
    "got": {
      "optional": true
    },
    "node-fetch": {
      "optional": true
    }
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  }
